<div id="mc_embed_signup" class="mc_embed_signup">
<form action="//sl2017.us12.list-manage.com/subscribe/post?u=ff41604c0441f9e27f87a47e2&amp;id=c1834121e4" method="post" id="mc-embedded-subscribe-form" name="mc-embedded-subscribe-form" class="validate" target="_blank" novalidate>
    <div id="mc_embed_signup_scroll">

        <div class="mc-field-group">
            <input type="email" value="" name="EMAIL" class="required email mc-input" id="mce-EMAIL" placeholder="Email Adresse">
        </div>
        <div class="mc-field-group">
            <input type="text" value="" name="FNAME" class="required mc-input" id="mce-FNAME" placeholder="Fornavn">
        </div>
        <div id="mce-responses" class="clear">
            <div class="response" id="mce-error-response" style="display:none"></div>
            <div class="response" id="mce-success-response" style="display:none"></div>
        </div>    <!-- real people should not fill this in and expect good things - do not remove this or risk form bot signups-->
        <div style="position: absolute; left: -5000px;" aria-hidden="true"><input type="text" name="b_ff41604c0441f9e27f87a47e2_c1834121e4" tabindex="-1" value=""></div>
        <div class="mc-field-group"><input type="submit" value="{% trans with {'context': 'mailchimp'} %}sign me up{% endtrans %}" name="subscribe" id="mc-embedded-subscribe" class="button mc-submit"></div>
    </div>
</form>
</div>
{{ attach_library('camp/mailchimp') }}
